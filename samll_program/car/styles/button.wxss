.button {
    display: inline-block;
    margin: 4px;
    background-color: #e0e1e2;
    padding: 0 24px;
    color: rgb(0,0,0,0.6);
    font-weight: bold;
    border-radius: 1px;
    font-size: 32rpx;
    transition: 0.3s all;
}
.primary {
    background-color: #000000;
    color: rgb(255,255,255,0.9);
}